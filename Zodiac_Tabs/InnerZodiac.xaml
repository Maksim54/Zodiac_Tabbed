<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Zodiac_Tabs.InnerZodiac"
             Title="A"
             x:Name="page">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Fill">
            <Image x:Name="img"></Image>
            <Label x:Name="date" Text="Dates" HorizontalOptions="Center" TextColor="GhostWhite"  FontSize="Large"></Label>
            <Label x:Name="dateA" HorizontalOptions="Center" TextColor="GhostWhite"></Label>
            <Label x:Name="traits" Text="Traits" HorizontalOptions="Center" TextColor="GhostWhite" FontSize="Large"></Label>
            <Label x:Name="traitA" HorizontalOptions="Center"  TextColor="GhostWhite"></Label>
            <Label x:Name="gem" Text="Lucky Gem" HorizontalOptions="Center" TextColor="GhostWhite" FontSize="Large"></Label>
            <Label x:Name="gemA" HorizontalOptions="Center" TextColor="GhostWhite"></Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>